{
  "compilerOptions": {
      "target": "es2022",
      "module": "ES2022",
      "strict": true,
      "noEmit": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node"]
  },
  "exclude": ["node_modules"]
}